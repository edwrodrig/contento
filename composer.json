{
    "name": "edwrodrig/contento",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.2",
        "ext-json": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\contento\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\contento\\" : "tests/",
            "test\\edwrodrig\\contento\\" : "tests/src"
         }
    },
    "archive" : {
        "exclude": [
            "/examples",
            "/tests",
            "/.idea",
            ".gitignore",
            ".git",
            "phpunit.xml.dist",
            "README.md",
            "/contento"
        ]
    }
}
